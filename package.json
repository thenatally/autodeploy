    "serve": "nodemon --env-file=.env ./dist/index.js"
  },
  "dependencies": {
    "body-parser": "latest",
    "chalk": "^5.4.1",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/body-parser": "latest",
    "@types/express": "^5.0.3",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.0.1",
    "express": "^5.1.0",
    "nodemon": "^3.0.0",
    "typescript": "^5.8.3"
  },
  "type": "module",
  "tpm": {
    "aliases": {
      "b": "build",
      "s": "start"
    }
  }
}
